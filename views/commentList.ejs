<!DOCTYPE html>
<html>
<head>
    <title>??-点评列表</title>
    <%- include('common/pageConfig') %>
    <!-- Custom styles for this template-->
    <link rel="stylesheet" href="/public/css/commentList.css">
</head>
<body class="layui-layout-body">
<div class="layui-layout layui-layout-admin">
    <%- include('common/header') %>
    <%- include('common/navigation') %>
    <Main>
        <div class="layui-body">
            <!-- 内容主体区域 -->
            <div class="layui-fluid">
                <div class="layui-row layui-col-space20">
                    <div class="layui-col-xs12 layui-col-sm12 layui-col-md12">
                        <div class="layui-card">
                            <div class="layui-card-body">
                                <div class="layui-row">
                                    <div class="layui-col-md2">
                                        <div class="table-title">
                                            点评列表
                                        </div>
                                    </div>
                                    <div class="layui-col-md4 layui-col-md-offset6">
                                        <div class="table-search">
                                            <form class="layui-form" action="">
                                                <div class="layui-form-item">
                                                    <div class="layui-input-block">
                                                        <input type="text" name="search" placeholder="搜索" autocomplete="off" class="layui-input" id="search">
                                                        <input type="text" style="display: none;">
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                <div class="layui-row">
                                    <div class="layui-col-md12">
                                        <table id="result" lay-filter="result"></table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </Main>
</div>
<script type="text/html" id="toolbar">
    <!--{{#  if(d.uploadStatus == 'false' && d.reportId){ }}-->
    <!--<a class="table-btn" lay-event="comment" href="/comment/{{d.reportId}}">点评</a>-->
    <!--{{#  } else { }}-->
    <!--<a class="table-btn table-btn-disable">点评</a>-->
    <!--{{#  } }}-->
    <a class="table-btn upload_btn_{{d.reportId}}" lay-event="upload">上传</a>
    <a class="table-btn" lay-event="download" data-href="http://localhost:8080/comment/download?reportId={{d.reportId}}">下载</a>
    <a class="table-btn" lay-event="submission">提交</a>
</script>
</body>
<%- include('common/script') %>
<script src="/public/js/commentList.js" type="application/javascript"></script>
</html>
